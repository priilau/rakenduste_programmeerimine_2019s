{
    "env": {
        "browser": true,
        "node": true,
    },
    "rules": {
        "no-console": "off",
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2019,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "settings": {
        "react": {
          "version": "detect"
        }
    },
}